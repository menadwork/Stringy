{
  "name": "voku/stringy",
  "description": "A string manipulation library with multibyte support",
  "keywords": [
    "multibyte",
    "string",
    "manipulation",
    "utility",
    "methods",
    "utf-8",
    "helpers",
    "utils",
    "utf"
  ],
  "homepage": "https://github.com/danielstjules/Stringy",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel St. Jules",
      "email": "danielst.jules@gmail.com",
      "homepage": "http://www.danielstjules.com",
      "role": "Maintainer"
    },
    {
      "name": "Lars Moelleken",
      "email": "lars@moelleken.org",
      "homepage": "http://www.moelleken.org/",
      "role": "Fork-Maintainer"
    }
  ],
  "repositories": {
    "portable-utf8": {
      "type": "vcs",
      "url": "https://github.com/menadwork/portable-utf8"
    },
    "urlify": {
      "type": "vcs",
      "url": "https://github.com/menadwork/urlify"
    },
    "anti-xss": {
      "type": "vcs",
      "url": "https://github.com/menadwork/anti-xss"
    },
    "email-check": {
      "type": "vcs",
      "url": "https://github.com/menadwork/email-check"
    }
  },
  "require": {
    "php": ">=5.3.0",
    "portable-utf8": "~3.1",
    "urlify": "~2.2",
    "anti-xss": "~2.1",
    "email-check": "~1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0|~5.0"
  },
  "support": {
    "issues": "https://github.com/voku/Stringy/issues",
    "source": "https://github.com/voku/Stringy"
  },
  "autoload": {
    "psr-4": {
      "Stringy\\": "src/"
    },
    "files": [
      "src/Create.php"
    ]
  }
}
